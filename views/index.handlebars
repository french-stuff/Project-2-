<section class="cta">
  <div class="cta-content">
    <div class="container" id="login">
      <h2>Stop waiting.<br>Start now.</h2>
      <a href="#myModal" data-toggle="modal" class="trigger-btn btn btn-outline btn-xl">Login
        today!</a>
    </div>
  </div>
  <div class="overlay"></div>

  <div id="myModal" class="modal fade">
    <div class="modal-dialog modal-login">
      <div class="modal-content">
        <div class="modal-header">
          <h4 class="modal-title">Member Login</h4>
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        </div>
        <div class="modal-body">
          
            <div class="form-group">
              <i class="fa fa-user"></i>
              <input id="userLog" type="text" class="form-control" placeholder="Username" required="required">
            </div>
            <div class="form-group">
              <i class="fa fa-lock"></i>
              <input id="passwordLog" type="password" class="form-control" placeholder="Password" required="required">
            </div>
            <span id="messageLog"></span>
            <div class="form-group">
              <button id="submitLog" class="btn btn-primary btn-block btn-lg">Log in</button>
            </div>
            <div class="modal-footer">
              <a id="reslogChange" href="#newUserModal" data-dismiss="modal" data-toggle="modal">Create an account</a>
            </div>
        </div>
      </div>
    </div>
  </div>

  <div id="newUserModal" class="modal fade">
    <div class="modal-dialog modal-login">
      <div class="modal-content">
        <div class="modal-header">
          <h4 class="modal-title">Create Account</h4>
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        </div>
        <div class="modal-body">
          
            <div class="form-group">
              <i class="fa fa-user"></i>
              <input id="userRes" type="text" class="form-control" placeholder="Username" required="required">
            </div>
            <div class="form-group">
              <i class="fa fa-lock"></i>
              <input id="passwordRes" type="password" class="form-control" placeholder="Password" required="required">
            </div>
            <span id="messageRes"></span>
            <div class="form-group">
              <button id="submitRes" class="btn btn-primary btn-block btn-lg">Create</button>
            </div>
            <div class="modal-footer">
              <a href="#myModal" data-dismiss="modal" data-toggle="modal">Already Have an Account?</a>
            </div>
         

        </div>
      </div>
    </div>
  </div>
</section>

